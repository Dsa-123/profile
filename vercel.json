{
  "version": 2,
  "builds": [
    {
      "src": "api/visits.js",
      "use": "@vercel/node"
    },
    {
      "src": "n/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/visits",
      "dest": "/api/visits"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ]
}
